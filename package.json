{
  "name": "new",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "concurrently \"npm run build:next\" \"npm run build:worker\"",
    "start": "concurrently \"npm run start:next\" \"npm run start:worker\"",
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:worker\"",
    "lint": "npm run lint:next && npm run lint:worker",
    "build:next": "cd main && next build",
    "start:next": "cd main && next start",
    "dev:next": "cd main && next dev",
    "lint:next": "cd main && next lint",
    "build:worker": "cd worker && esbuild src/worker.ts --platform=node --bundle --minify --outfile=dist/worker.js",
    "start:worker": "npm run build:worker && node --require dotenv/config worker/dist/worker.js",
    "dev:worker": "npm run build:worker && nodemon --require dotenv/config worker/dist/worker.js",
    "lint:worker": "cd worker && eslint"
  },
  "dependencies": {
    "@colony/colony-js": "^7.0.2",
    "@colony/sdk": "^2.0.0-next.7",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.4",
    "@prisma/client": "^5.7.1",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@wagmi/chains": "^1.8.0",
    "@wagmi/core": "^1.3.9",
    "autoprefixer": "10.4.14",
    "bootstrap": "^5.3.1",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.10",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.13",
    "ethers": "^5.7.2",
    "html-entities": "^2.4.0",
    "next": "^14.0.4",
    "next-auth": "^4.23.0",
    "postcss": "8.4.27",
    "prisma": "^5.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "socket.io": "^4.7.2",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "viem": "^1.6.0",
    "wagmi": "^1.3.10"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "concurrently": "^8.2.2",
    "express": "^4.18.2",
    "nodemon": "^3.0.2"
  }
}
