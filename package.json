{
  "name": "new",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "concurrently \"yarn run build:next\" \"yarn run build:worker\"",
    "start": "concurrently \"yarn run start:next\" \"yarn run start:worker\"",
    "dev": "concurrently \"yarn run dev:next\" \"yarn run dev:worker\"",
    "lint": "yarn run lint:next && yarn run lint:worker",
    "build:next": "cd main && next build",
    "start:next": "cd main && next start",
    "dev:next": "cd main && next dev",
    "lint:next": "cd main && next lint",
    "build:worker": "cd worker && esbuild src/worker.ts --platform=node --bundle --minify --outfile=dist/worker.js --sourcemap  ",
    "start:worker": "yarn run build:worker && node --require dotenv/config worker/dist/worker.js",
    "dev:worker": "tsx worker/src/worker.ts",
    "lint:worker": "cd worker && eslint"
  },
  "dependencies": {
    "@colony/sdk": "^3.2.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.2.0",
    "@porton/carbon-flow": "git+https://github.com/vporton/carbon-flow.git#main",
    "@prisma/client": "^6.12.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@tanstack/react-query": "^5.83.0",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@wagmi/chains": "^1.8.0",
    "@wagmi/core": "^2.18.0",
    "autoprefixer": "10.4.21",
    "bootstrap": "^5.3.7",
    "dotenv": "^17.2.0",
    "esbuild": "^0.25.8",
    "eslint": "9.31.0",
    "eslint-config-next": "15.4.2",
    "ethers": "^5.8.0",
    "html-entities": "^2.6.0",
    "next": "^15.4.2",
    "next-auth": "^4.24.11",
    "pino-pretty": "^13.0.0",
    "postcss": "8.5.6",
    "prisma": "^6.12.0",
    "react": "19.1.0",
    "react-datepicker": "^8.4.0",
    "react-dom": "19.1.0",
    "react-router-dom": "^7.7.0",
    "socket.io": "^4.8.1",
    "tailwindcss": "4.1.11",
    "typescript": "5.8.3",
    "viem": "2.33.0",
    "wagmi": "^2.16.0"
  },
  "devDependencies": {
    "@prisma/nextjs-monorepo-workaround-plugin": "^6.12.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.15",
    "@types/react-datepicker": "^7.0.0",
    "concurrently": "^9.2.0",
    "express": "^5.1.0",
    "nodemon": "^3.1.10"
  }
}
